{
    "version": 2,
    "builds": [
        {
            "src" : "src/main.ts",
            "use" : "@vercel/static-build"
        }
        
    ],
    "routes": [
        {
            "src" : "/(.*)",
            "dest" : "src/main.ts",
            "methods" : [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}